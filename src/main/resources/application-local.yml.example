# 로컬 개발 환경 설정 파일 예제
# 이 파일을 복사하여 application-local.yml로 만들고 실제 값으로 채워주세요
# application-local.yml은 .gitignore에 포함되어 Git에 올라가지 않습니다

spring:
  datasource:
    url: jdbc:mysql://YOUR_DB_HOST:3306/YOUR_DB_NAME?useSSL=false&serverTimezone=Asia/Seoul&allowPublicKeyRetrieval=true
    username: YOUR_DB_USERNAME
    password: YOUR_DB_PASSWORD
    driver-class-name: com.mysql.cj.jdbc.Driver

app:
  elasticsearch:
    host: http://YOUR_ELASTICSEARCH_HOST:9200
    username: YOUR_ELASTICSEARCH_USERNAME
    password: YOUR_ELASTICSEARCH_PASSWORD
  alpha-vantage:
    api-key: YOUR_ALPHA_VANTAGE_API_KEY
  data-supply-api:
    datasource:
      url: jdbc:mysql://YOUR_API_DB_HOST:3306/YOUR_API_DB_NAME?useSSL=false&serverTimezone=Asia/Seoul&allowPublicKeyRetrieval=true
      username: YOUR_API_DB_USERNAME
      password: YOUR_API_DB_PASSWORD
      driver-class-name: com.mysql.cj.jdbc.Driver

